	left: 0;
	top: 0;
	width: 200px;
	height: 100%;
	background: #fff;
	box-shadow: 0 0 2px rgba(0, 0, 0, 0.3);
}
.mainbox:after {
	content: '';
	position: absolute;
	right: 0;
	top: 0;
	width: 220px;
	height: 100%;
	background: #fff;
	box-shadow: 0 0 2px rgba(0, 0, 0, 0.3);
}
